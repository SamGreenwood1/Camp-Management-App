# Environment Variables for Camp Management App
# Copy this file to .env.local and modify as needed

# App Configuration
NODE_ENV=development
VITE_APP_NAME="Camp Management App"
VITE_APP_VERSION=1.0.0

# API Configuration (when backend is added)
# VITE_API_URL=http://localhost:3000/api
# VITE_API_KEY=your_api_key_here

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false

# Scheduling Algorithm Configuration
NO_REPEATS_DAYS=3
ALLOWED_TRANSITION_TIME=30
